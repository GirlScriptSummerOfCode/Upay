{
  "name": "with-jest",
  "version": "1.0.0",
  "dependencies": {
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "22.4.3",
    "express": "^4.16.3",
    "next": "^6.0.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.9.1",
    "husky": "^0.14.3",
    "jest": "^22.0.1",
    "lint-staged": "^7.2.0",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "16.2.0"
  },
  "scripts": {
    "prepush": "npm test",
    "test": "NODE_ENV=test jest",
    "dev": "node server",
    "build": "next build",
    "start": "NODE_ENV=production node server"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ]
      }
    }
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "tabWidth": 2,
    "useTabs": false
  }
}
